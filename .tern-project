{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "chai"
  ],
  "loadEagerly": [],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "doc_comment": true,
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "complete_strings": {
      "maxLength": 15
    },
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    }
  }
}